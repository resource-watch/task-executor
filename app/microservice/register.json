		"authenticated": "true",
		"redirect": {
			"method": "DELETE",
			"path": "/api/v1/task/:id"
		}
	}, {
		"path": "/v1/task/sync-dataset",
		"method": "POST",
		"authenticated": "true",
		"redirect": {
			"method": "POST",
			"path": "/api/v1/task/sync-dataset"
		}
	}, {
		"path": "/v1/task/sync-dataset/by-dataset",
		"method": "PUT",
		"authenticated": "true",
		"redirect": {
			"method": "PUT",
			"path": "/api/v1/task/sync-dataset/by-dataset"
		}
	}]
}
